{
  "name": "pug-starter",
  "version": "1.0.0",
  "description": "Simple pug (jade) starter aimed to quickly deliver HTML & CSS projects",
  "main": "index.js",
  "scripts": {
    "dev": "gulp pug",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "//": "CUSTOM CONFIGURATION",
  "config": {
    "directories": {
      "development": "tmp",
      "production": "build",
      "ghpages": "docs",
      "source": "src",
      "data": "_data",
      "component": "_component",
      "asset": "_asset",
      "image": "image",
      "font": "font"
    },
    "entries": {
      "cssExternal": "style**.{scss,sass}",
      "cssInline": "inline**.{scss,sass}",
      "css": {
        "inline": true,
        "external": true
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marianzburlea/pug-starter.git"
  },
  "keywords": [
    "pug",
    "starter",
    "jade",
    "starter"
  ],
  "author": "Marian Zburlea",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marianzburlea/pug-starter/issues"
  },
  "homepage": "https://github.com/marianzburlea/pug-starter#readme",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.16.1",
    "foldero": "^0.1.1",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-pug": "^3.2.0",
    "minimist": "^1.2.0"
  }
}
